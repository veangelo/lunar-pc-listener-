<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="root" />
        <exclude domain="file" path="AppData"/>
        <exclude domain="database" path="AppData"/>
        <exclude domain="sharedpref" path="AppData"/>
        <exclude domain="external" path="AppData"/>
    </cloud-backup>
    <device-transfer>
        <include domain="root" />
        <exclude domain="file" path="AppData"/>
        <exclude domain="database" path="AppData"/>
        <exclude domain="sharedpref" path="AppData"/>
        <exclude domain="external" path="AppData"/>
    </device-transfer>
</data-extraction-rules>